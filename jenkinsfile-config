// Definitions here
    def COLOR_MAP = [
        'SUCCESS': 'good',
        'FAILURE': 'danger',
    ]

pipeline {

    agent any

    stages {
        stage('Git Checkout') {
            steps {
                git branch: 'feature1', credentialsId: 'jenkins-github-integration', url: 'https://github.com/MFresh/funcalc.git'
            }
        }

        stage('Gradle Build') {
            steps {
                sh './gradlew build'
            }
        }

        stage('Slack it'){
            steps {
                 slackSend channel: '#builds',
                 message: 'Started Build ${JOB_NAME} with build ${BUILD_NUMBER}'
            }
        }


    }
}
